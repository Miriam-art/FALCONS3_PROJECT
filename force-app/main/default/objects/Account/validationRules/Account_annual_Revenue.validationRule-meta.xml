<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_annual_Revenue</fullName>
    <active>false</active>
    <errorConditionFormula>OR(AnnualRevenue &lt; 0, AnnualRevenue &gt; 100
					
					
					)</errorConditionFormula>
    <errorMessage>Annual Revenue cannot exceed 100 billion</errorMessage>
</ValidationRule>
